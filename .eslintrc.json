{
  "extends": "airbnb-base",
  "plugins": [
    "jest"
  ],
  "env": {
    "jest/globals": true
  },
  "globals": {
    "window": true,
    "dayjs": true
  },
  "rules": {
    "semi": [0],
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-param-reassign": [
      0
    ],
    "func-names": [
      0
    ],
    "import/no-extraneous-dependencies": [
      0
    ],
    "import/no-unresolved": [
      2,
      {
        "ignore": [
          "dayjs"
        ]
      }
    ],
    "import/extensions": [
      2,
      "ignorePackages"
    ],
    "no-bitwise": 0,
    "default-case": 0,
    "consistent-return": 0,
    "linebreak-style": 0
  }
}