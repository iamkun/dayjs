{
  "name": "dayjs",
  "version": "2.0.0-alpha.2",
  "packageManager": "pnpm@7.1.5",
  "description": "2KB immutable date time library alternative to Moment.js with the same modern API.",
  "keywords": [
    "dayjs",
    "date"
  ],
  "license": "MIT",
  "homepage": "https://day.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamkun/dayjs.git"
  },
  "author": "iamkun",
  "contributors": [
    {
      "name": "三咲智子",
      "email": "sxzz@sxzz.moe",
      "url": "https://github.com/sxzz"
    }
  ],
  "funding": "https://opencollective.com/dayjs",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "unpkg": "./dist/index.iife.min.js",
  "jsdelivr": "./dist/index.iife.min.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && run-p build:src build:dts",
    "build:src": "tsx build/index.mts",
    "build:dts": "tsc -p tsconfig.src.json --emitDeclarationOnly",
    "size": "pnpm run build:src && size-limit",
    "lint": "eslint . --ext .js,.ts,.md,.json",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier . --write",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "bench": "vitest bench",
    "typecheck": "run-p typecheck:*",
    "typecheck:src": "tsc --noEmit -p tsconfig.src.json --composite false",
    "typecheck:test": "tsc --noEmit -p tsconfig.vitest.json --composite false",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false"
  },
  "dependencies": {
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@size-limit/file": "^7.0.8",
    "@sxzz/eslint-config-prettier": "^2.2.2",
    "@sxzz/eslint-config-ts": "^2.2.2",
    "@types/node": "*",
    "c8": "^7.11.3",
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "esbuild": "^0.14.48",
    "eslint": "^8.19.0",
    "eslint-define-config": "^1.5.1",
    "fast-glob": "^3.2.11",
    "moment": "^2.29.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "size-limit": "^7.0.8",
    "terser": "^5.14.1",
    "tsx": "^3.7.1",
    "typescript": "^4.7.4",
    "unplugin-ast": "^0.5.2",
    "vitest": "^0.27.1"
  },
  "engines": {
    "node": ">=14.19.0"
  },
  "size-limit": [
    {
      "limit": "2.99 KB",
      "path": "dist/index.min.js"
    },
    {
      "limit": "2.99 KB",
      "path": "dist/index.min.mjs"
    },
    {
      "limit": "2.99 KB",
      "path": "dist/index.iife.min.js"
    }
  ]
}
